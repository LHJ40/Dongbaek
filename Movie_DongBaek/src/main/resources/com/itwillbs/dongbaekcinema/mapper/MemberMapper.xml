<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itwillbs.dongbaekcinema.mapper.MemberMapper">
<!--   <cache-ref namespace=""/> -->

	<!-- 관리자 페이지 - 회원 한명 id로 조회   -->
	<select id="selectStudent" resultType="com.itwillbs.dongbaekcinema.vo.MemberVO">
		select * 
		from members
		where id = #{id}
	</select>
	
	<!-- 관리자페이지 - 회원 전체 조회해서 뿌리기 -->
	<select id="selectMemberList" resultType="com.itwillbs.dongbaekcinema.vo.MemberVO">
		select *
		from members
	</select>
	
	
	
</mapper>
